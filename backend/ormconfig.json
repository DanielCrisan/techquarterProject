        "compiled/ds/subscriber/**/*.js"
    ],
    "cli": {
        "entitiesDir": "compiled/ds/models",
        "migrationsDir": "compiled/ds/migration",
        "subscribersDir": "compiled/ds/subscriber"
    }
}