{
    "type": "sqlite",
    "database": "team3_database.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
        "compiled/ds/models/**/*.js"
    ],
    "migrations": [
        "compiled/ds/migration/**/*.js"
    ],
